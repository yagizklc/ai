    "import torch\n",
    "\n",
    "# Create a tensor with gradient tracking\n",
    "x = torch.tensor([2.0], requires_grad=True)\n",
    "y = x**5 + 1  # Simple operation\n",
    "y.backward()  # Compute gradients\n",
    "\n",
    "print(x.grad)  # Will show derivative of y with respect to x"
   ]
  }
